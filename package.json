{
  "name": "javascipt-hello-world",
  "version": "1.0.0",
  "description": "JavaScript Hello World",
  "scripts": {
    "prestart": "node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel app test lint:watch test:watch",
    "audit": "npm-audit-ci",
    "app": "node buildScripts/srcServer.js",
    "share": "lt --port 3000",
    "lint": "esw buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress \"buildScripts/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "WHOKnows",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0",
    "body-parser": "^1.16.2",
    "express": "4.17.0"
  },
  "devDependencies": {
    "audit-ci": "^2.0.1",
    "git-rev-sync": "^1.12.0",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-watch": "5.1.2",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5"
  }
}
